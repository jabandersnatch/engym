    "import numpy as np\n",
    "\n",
    "def calculate_minimum_force(r_bar, s_y):\n",
    "    \"\"\"\n",
    "    Calculate the minimum force that the bar must have to yield.\n",
    "    \"\"\"\n",
    "    n_stress = s_y\n",
    "    t_force = n_stress * r_bar ** 2 * np.pi \n",
    "    return t_force\n",
    "\n",
    "calculate_minimum_force(0.012, 250e6)\n"
   ]
  }
